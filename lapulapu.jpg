  <script>
      function hasSideBarNav() {
          const navMenu = document.getElementById('side-bar-nav');
          return (navMenu && window.getComputedStyle(navMenu).display === 'block');
      }

      // Fixes the layout for expanded side bar
      if (hasSideBarNav() && JSON.parse(localStorage.getItem('side-bar-opened'))) {
          document.getElementById('app').classList.add('side-bar-opened');
          document.getElementById('sideBar').classList.add('expanded');
      }

      // Fixes the layout for collapsed side bar
      const sideBar = document.querySelector('#sideBar');
      if (sideBar) {
          document.getElementById('app').classList.toggle('side-bar-shown');
      }
  </script>

